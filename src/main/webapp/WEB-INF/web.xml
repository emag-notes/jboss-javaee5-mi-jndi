<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>jboss-javaee5-mi-jndi</display-name>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>dbconn</display-name>
    <servlet-name>dbconn</servlet-name>
    <servlet-class>org.study.java.jboss.javaee5.mi.jndi.DBConnServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>dbconn</servlet-name>
    <url-pattern>/dbconn</url-pattern>
  </servlet-mapping>
</web-app>